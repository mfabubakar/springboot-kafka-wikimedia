            <div th:unless="${not #lists.isEmpty(inputDtos)}">
                <div th:if="${inputDto.delivaryGroup != null }">
                    <h4 th:text="'The Delivery Group: '+ ${inputDto.delivaryGroup} + ' is not setup in CXFII for above Msg Type'"></h4>
                </div>
            </div>
        </div>
    </form>
</div>

