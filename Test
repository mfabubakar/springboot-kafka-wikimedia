package com.abu.cxf.model;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;
import lombok.ToString;

//import javax.validation.constraints.NotBlank;
//import javax.validation.constraints.NotEmpty;
//import javax.validation.constraints.Size;
import java.sql.Date;
import java.util.List;
import java.util.Map;

@Data
@NoArgsConstructor
@AllArgsConstructor
@ToString
public class InputDto {
    private int row_count;
    public String region;
    private String delivaryGroup;
    private String qualifier;
    private String desc;
    public int cr1Number;
    public int cd1Number;
    public String msgtype;
    public String formatCd;
    public String prodCode;
    public String variations;
    public Date effectiveDate;
    public Date expireDate;
    public String brokerPresentCd;
    public String secPresCd;
    public String testInd;
    public String eventDriven;
    public String accPresCd;

    //    @NotBlank(message = "Security account should be entered")
//    @Size(min=4, max=40)
//    @NotEmpty(message = "Security account should be entered")
    private String account;
    private String shortName;
    private String actStatus;

    ////    @NotBlank(message = "cash account should be entered")
    private String cashAccount;
    private String cashAccountBranch;
    private String cashAccountname;
    private String cashAccountCloseDate;

    private int schdno;
    private String schdfreqcd;
    private String schdcob;
    private Date schdeffcdt;
    private String schddesc;

    private String detDesc;
    private String detOption;
    private String detLongDesc;

    private List<Map<String, Object>> dataList;   
}


package com.abu.cxf.model;

