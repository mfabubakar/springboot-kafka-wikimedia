    @PostMapping("/cxfbyCashACT")
    public String showCashActForm(@ModelAttribute("inputDto") InputDto inputDto, Model model,
                                  BindingResult result,
                                  RedirectAttributes attr, HttpSession session) {
        System.out.println(inputDto.getCashAccount());
        System.out.println(inputDto.getRegion());
//        if (result.hasErrors()) {
//            List<String> listRegion = Arrays.asList("GSSPROD");
//            model.addAttribute("listRegion", listRegion);
//            return "Enq-cac-act";
//        }

        List<String> listRegion = Collections.singletonList(inputDto.getRegion());
        model.addAttribute("listRegion", listRegion);

        if (inputDto.getCashAccount() != null && inputDto.getRegion() != null) {
            System.out.println("before call->" + inputDto.getRegion());
            String inRegion = inputDto.getRegion();
            String inCashAccount = inputDto.getCashAccount();
            model.addAttribute("inputDtos", cxfDao.getmainRowCashAct(inputDto));
            inputDto.setRegion(inRegion);
            inputDto.setCashAccount(inCashAccount);
            model.addAttribute("inputDto", inputDto);
//            System.out.println("abu" + cxfDao.getmainRow(inputDto));
        }
        return "Enq-cac-act";
    }
