@Controller
public class FirstScreenController {
    @Autowired
    CxfDao cxfDao;

    @GetMapping("/cxfmenu")
    public String showForm(Model model) {
        InputDto inputDto = new InputDto();
        model.addAttribute("inputDto", inputDto);

//        List<String> listRegion = Arrays.asList("GSSUNIT", "GSSSYST", "GSSETST", "GSSDSIT", "GSSCREM"
//                , "GSSE2E", "GSSCFP", "GSSCFP2", "GSSCFP3", "GSSCFP4", "GSSTEST");
        List<String> listRegion = Arrays.asList("GSSPROD");
        model.addAttribute("listRegion", listRegion);

        List<String> listofBICS = new ArrayList<>();
        List<String> listQual = new ArrayList<>();
        return "menu_form";
    }

    @PostMapping("/cxfmenu")
    public String showForm(@ModelAttribute("inputDto") InputDto inputDto, Model model) {
//        if (inputDto.getDelivaryGroup() == null || inputDto.getDelivaryGroup().equals("")) {
        if (inputDto.getDelivaryGroup().equals("")) {
            List<Map<String, Object>> listMap = cxfDao.getUniqueBICS(inputDto);
            List<String> listofBICS = new ArrayList<String>();
            for (Map<String, Object> m : listMap) {
                for (Map.Entry<String, Object> e : m.entrySet()) {
                    listofBICS.add((String) e.getValue());
                }
            }
            model.addAttribute("listofBICS", listofBICS);
//            System.out.println("list of bics : " + listofBICS);
        }
        List<String> listRegion = Collections.singletonList(inputDto.getRegion());
        model.addAttribute("listRegion", listRegion);

//        if (inputDto.getRegion() == null && inputDto.getDelivaryGroup().equals("")) {
//            List<Map<String, Object>> listQuals = cxfDao.getQualbyBic(inputDto);
//            List<String> listQual = new ArrayList<String>();
////            listQual.add("--ALL--");
//            for (Map<String, Object> m : listQuals) {
//                for (Map.Entry<String, Object> e : m.entrySet()) {
//                    listQual.add((String) e.getValue());
//                }
//            }
//            model.addAttribute("listQual", listQual);
//            List<String> listofBICS = Collections.singletonList(inputDto.getDelivaryGroup());
//            model.addAttribute("listofBICS", listofBICS);
//        }

        if (!inputDto.getDelivaryGroup().equals("") && inputDto.getRegion() != null) {
//            System.out.println("before call->" + inputDto.getRegion());
            String inRegion = inputDto.getRegion();
            String inDelv = inputDto.getDelivaryGroup();
            String inmsgtype = inputDto.getMsgtype();
//            System.out.println("msgtype" + inmsgtype);

            model.addAttribute("inputDtos", cxfDao.getmainRow(inputDto));
            inputDto.setRegion(inRegion);
            inputDto.setDelivaryGroup(inDelv);
            model.addAttribute("inputDto", inputDto);
//            System.out.println("abu" + cxfDao.getmainRow(inputDto));
//            List<String> listofBICS = Collections.singletonList(inputDto.getDelivaryGroup());
//            model.addAttribute("listofBICS", listofBICS);
            List<Map<String, Object>> listMap = cxfDao.getUniqueBICS(inputDto);
            List<String> listofBICS = new ArrayList<String>();
            for (Map<String, Object> m : listMap) {
                for (Map.Entry<String, Object> e : m.entrySet()) {
                    listofBICS.add((String) e.getValue());
                }
            }
            model.addAttribute("listofBICS", listofBICS);
        }
        return "menu_form";
    }

    @GetMapping("/cxf/view/{cr1Number}/{region}")
    public String getAllRows(@PathVariable int cr1Number, @PathVariable String region,
                             @ModelAttribute("inputDto") InputDto inputDto, Model model) {
//        System.out.println(cr1Number);
//        System.out.println(region);
//        System.out.println(cxfDao.getBicQualRows(cr1Number));

        model.addAttribute("inputDtos", cxfDao.getBicQualRows(cr1Number, region));
        inputDto.setRegion(region);
        model.addAttribute("inputDto", inputDto);
        return "view-cxf-setup";
    }

    //    http://localhost:8080/cxf/update/GSSUNIT/14479/29707

    @GetMapping("/cxf/update/{region}/{cr1Number}/{cd1Number}")
    public String cxfUpdateForm(@PathVariable String region, @PathVariable int cr1Number, @PathVariable int cd1Number,
                                @ModelAttribute("inputDto") InputDto inputDto, Model model) {
//        System.out.println("inside update-->" + "cr1Number : " + cr1Number + "cd1Number : " + cd1Number+region);

        model.addAttribute("ViewUpdateDto", cxfDao.getBicQualRow(region, cr1Number, cd1Number));
//        inputDto.setRegion(region);

        return "update-cxf-setup";
    }

    @PostMapping("/cxf/update/{region}/{cr1Number}/{cd1Number}")
    public String cxfUpdateForms(@PathVariable String region, @PathVariable int cr1Number, @PathVariable int cd1Number,
                                 @ModelAttribute("inputDto") InputDto inputDto, Model model) {
//        System.out.println("inside update-->" + "cr1Number : " + cr1Number + "cd1Number : " + cd1Number);
//        System.out.println("inputDto-->" +inputDto);
        int n = cxfDao.updateSetup(inputDto);
        System.out.println("rows updated : " + n);
        model.addAttribute("inputDto", inputDto);

        return "update-cxf-setup";

    }

    @GetMapping("/cxf/secacct/{region}/{cr1Number}")
    public String getAllSecAccts(@PathVariable int cr1Number, @PathVariable String region,
                                 @ModelAttribute("inputDto") InputDto inputDto, Model model) {
        System.out.println(cr1Number);
        System.out.println(region);
        System.out.println(cxfDao.getSecAccts(cr1Number, region));

        List<InputDto> list1 = cxfDao.getSecAccts(cr1Number, region);

        List<InputDto> list2 = cxfDao.getSecAcctsPos(cr1Number, region);

        List<InputDto> concatenated_list
                = new ArrayList<InputDto>();

        concatenated_list.addAll(list1);
        concatenated_list.addAll(list2);

        model.addAttribute("inputDtos", concatenated_list);

//        if (list1.isEmpty()) {
//            model.addAttribute("inputDtos", cxfDao.getSecAcctsPos(cr1Number, region));
//        } else {
//            model.addAttribute("inputDtos", cxfDao.getSecAccts(cr1Number, region));
//        }


//        inputDto.setRegion(region);
//        model.addAttribute("inputDto", inputDto);
//        return  cxfDao.getSecAccts(cr1Number, region);
        return "sec-acts-view";
    }

