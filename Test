package com.abu.cxf.dao;

import com.abu.cxf.model.CategoryDto;
import com.abu.cxf.model.DestinationDto;
import com.abu.cxf.model.InputDto;
import com.abu.cxf.model.ViewUpdateDto;
import java.util.List;
import java.util.Map;

public interface CxfDao {

    List<Map<String, Object>> getUniqueBICS(InputDto inputDto);

    List<Map<String, Object>> getQualbyBic(InputDto inputDto);

    List<Map<String, Object>> getSwiftRow(InputDto inputDto);

    List<InputDto> getmainRow(InputDto inputDto);

    List<InputDto> getmainRowAct(InputDto inputDto);

    List<InputDto> getmainRowCashAct(InputDto inputDto);

    List<InputDto> getBicQualRows(int cr1Number, String region);

    ViewUpdateDto getBicQualRow(String region, int cr1Number, int cd1Number);


    List<InputDto> getSecAccts(int cr1Number, String region);
    List<InputDto> getSecAcctsPos(int cr1Number, String region);

    List<InputDto> getCashAcct(int cr1Number, String region);

    List<InputDto> getSchedule(int cd1Number, String region);

    List<CategoryDto> getCategory(int cd1Number, String region);

    List<DestinationDto> getDestination(int cd1Number, String region);

    List<InputDto> getOptDetail(int cr1Number, int cd1Number, String region);

    int updateSetup(InputDto inputDto);
}
=================================================
package com.abu.cxf.dao;

import com.abu.cxf.model.CategoryDto;
import com.abu.cxf.model.DestinationDto;
import com.abu.cxf.model.InputDto;
import com.abu.cxf.model.ViewUpdateDto;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.jdbc.core.JdbcTemplate;
import org.springframework.stereotype.Component;

import javax.swing.text.View;
import java.util.List;
import java.util.Map;

@Component("dao")
public class CxfDAOImplementation implements CxfDao {

    @Autowired
    JdbcTemplate jdbcTemplate;

    @Override
    public List<Map<String, Object>> getUniqueBICS
            (InputDto inputDto) {

        String region = inputDto.getRegion().trim();
        String sql = "";

        sql = "SELECT DISTINCT(CR1_BUS_ID) FROM " + region +
                ".CLIENT_RPTG_GROUP1  \n" +
                "WITH UR;";
//        System.out.println("Unique Bics : " + sql);
        return jdbcTemplate.queryForList(sql);
    }

    @Override
    public List<Map<String, Object>> getQualbyBic(InputDto inputDto) {
        String region = inputDto.getRegion().trim();
        String delivaryGroup = inputDto.getDelivaryGroup().trim();
//        String msgType = inputDto.msgtype.trim();
        String sql = "";
        sql = "SELECT CR1_BUS_QLFY_ID FROM " + region +
                ".CLIENT_RPTG_GROUP1 WHERE CR1_BUS_ID = '" + delivaryGroup +
                "' WITH UR;";
        System.out.println(" Bic+Qual : " + sql);
        return jdbcTemplate.queryForList(sql);
    }

    @Override
    public List<Map<String, Object>> getSwiftRow(InputDto inputDto) {
        String region = inputDto.getRegion().trim();
        String delivaryGroup = inputDto.getDelivaryGroup().trim();
        String qulaf = inputDto.getQualifier().trim();
        String sql = "";
        sql = "select \n" +
                "SMG_TY_CD1\n" +
                ",PRF_CD_CD1\n" +
                ",SMV_CD_CD1\n" +
                ",CD1_EFF_DT\n" +
                ",CD1_EXP_DT\n" +
                ",CD1_TEST_IN\n" +
                ",CD1_EVENT_DRIVE_IN\n" +
                ",CD1_NO\n" +
                ",CR1_NO_CD1 \n" +
                "FROM " + region + ".CDV_EXTRACT1 where CR1_NO_CD1   IN( SELECT CR1_NO FROM " + region +
                ".CLIENT_RPTG_GROUP1 \n" +
                "\t\t\t\tWHERE  CR1_BUS_ID = '" + delivaryGroup +
                "'\t\t\t\t AND CR1_BUS_QLFY_ID = '" + qulaf + "'\t\t\t\t\t) WITH UR;";
//        System.out.println("sql-3" + sql);
        return jdbcTemplate.queryForList(sql);
    }

    @Override
    public List<InputDto> getmainRow(InputDto inputDto) {
        String region = inputDto.getRegion().trim();
        String delivaryGroup = inputDto.getDelivaryGroup().trim();
        String msgType = inputDto.getMsgtype().trim().toUpperCase();
        String prodCode = inputDto.getProdCode().trim().toUpperCase();
        String sql = "";
//        sql = "SELECT CR1_BUS_ID" +
//                ",CR1_BUS_QLFY_ID\n" +
//                ",CR1_DE\n" +
//                ",CR1_NO\n" +
//                "FROM " + region + ".CLIENT_RPTG_GROUP1\n" +
//                "WHERE  CR1_BUS_ID    =  '" + delivaryGroup + "' WITH UR";
        sql = "SELECT DISTINCT CR1_BUS_ID ,CR1_BUS_QLFY_ID ,CR1_DE, CR1_NO\n FROM " + region +
                ".CLIENT_RPTG_GROUP1 A\n " +
                "," + region + ".CDV_EXTRACT1\t\t\tB \t\n" +
                "WHERE A.CR1_NO = B.CR1_NO_CD1 \n" +
                "AND A.CR1_BUS_ID = '" + delivaryGroup + "' ";

        if (msgType != null && !msgType.equals("")) {
            sql = sql + "  AND B.SMG_TY_CD1 LIKE '" + msgType + "%'";
        }
        if (prodCode != null && !prodCode.equals("")) {
            sql = sql + "  AND B.PRF_CD_CD1 LIKE '" + prodCode + "%'";
        }
        sql = sql + " WITH UR";

        System.out.println("sql abu2->" + sql);
        return jdbcTemplate.query(
                sql,
                (rs, rowNum) ->
                        new InputDto(
                                rs.getString("CR1_BUS_ID"),
                                rs.getString("CR1_BUS_QLFY_ID"),
                                rs.getString("CR1_DE"),
                                rs.getInt("CR1_NO")
                        ));
    }

    @Override
    public List<InputDto> getmainRowAct(InputDto inputDto) {
        String region = inputDto.getRegion().trim();
        String account = inputDto.getAccount().trim();
        String msgType = inputDto.getMsgtype().trim().toUpperCase();
        String prodCode = inputDto.getProdCode().trim().toUpperCase();

        String sql = "";

        sql = "SELECT DISTINCT D.CR1_BUS_ID\n" +
                ",CR1_BUS_QLFY_ID\n" +
                ",DDE_LINE1_NM  \n" +
                ",CR1_NO_CS1\n" +
                "from " + region + ".DLV_DEST_EXT_LINK1 A\n" +
                ", " + region + ".CDV_EXTRACT1\t\t\tB \t\n" +
                ", " + region + ".CRG_SEC_ACCOUNT1\t\tC\n" +
                ", " + region + ".CLIENT_RPTG_GROUP1 \tD\n" +
                ", " + region + ".DLV_DESTINATION\n" +
                "WHERE A.CD1_NO_DL1 = B.CD1_NO\n" +
                "  AND C.CR1_NO_CS1 = B.CR1_NO_CD1\n" +
                "  AND D.CR1_NO \t   = C.CR1_NO_CS1 \n" +
                "  AND DDE_ID \t   = D.CR1_BUS_ID\n" +
                "  AND ACC_NO_CS1  =  '" + account + "' \n";

        if (msgType != null && !msgType.equals("")) {
            sql = sql + " AND B.SMG_TY_CD1 LIKE '" + msgType + "%'";
        }
        if (prodCode != null && !prodCode.equals("")) {
            sql = sql + "  AND B.PRF_CD_CD1 LIKE '" + prodCode + "%'";
        }
        sql = sql + " WITH UR";

        System.out.println("sql act1111->" + sql);
        return jdbcTemplate.query(
                sql,
                (rs, rowNum) ->
                        new InputDto(
                                rs.getString("CR1_BUS_ID"),
                                rs.getString("CR1_BUS_QLFY_ID"),
                                rs.getString("DDE_LINE1_NM"),
                                rs.getInt("CR1_NO_CS1")
                        ));
    }

    @Override
    public List<InputDto> getmainRowCashAct(InputDto inputDto) {
        String region = inputDto.getRegion().trim();
        String cashAccount = inputDto.getCashAccount().trim();
        String msgType = inputDto.getMsgtype().trim().toUpperCase();
        String prodCode = inputDto.getProdCode().trim().toUpperCase();
        String sql = "";

        sql = "SELECT DISTINCT D.CR1_BUS_ID\n" +
                ",CR1_BUS_QLFY_ID\n" +
                ",DDE_LINE1_NM  \n" +
                ",CR1_NO_CA1\n" +
                "from " + region + ".DLV_DEST_EXT_LINK1 A\n" +
                ", " + region + ".CDV_EXTRACT1\t\t\tB \t\n" +
                ", " + region + ".CRG_CASH_ACCOUNT1\t\tC\n" +
                ", " + region + ".CLIENT_RPTG_GROUP1 \tD\n" +
                ", " + region + ".DLV_DESTINATION\n" +
                "WHERE A.CD1_NO_DL1 = B.CD1_NO\n" +
                "  AND C.CR1_NO_CA1 = B.CR1_NO_CD1\n" +
                "  AND D.CR1_NO \t   = C.CR1_NO_CA1 \n" +
                "  AND DDE_ID \t   = D.CR1_BUS_ID\n" +
                "  AND CAC_NO_CA1  =  '" + cashAccount + "'";
        if (msgType != null && !msgType.equals("")) {
            sql = sql + " AND B.SMG_TY_CD1 LIKE '" + msgType + "%'";
        }
        if (prodCode != null && !prodCode.equals("")) {
            sql = sql + "  AND B.PRF_CD_CD1 LIKE '" + prodCode + "%'";
        }
        sql = sql + " WITH UR";

        System.out.println("sql act222->" + sql);
        return jdbcTemplate.query(
                sql,
                (rs, rowNum) ->
                        new InputDto(
                                rs.getString("CR1_BUS_ID"),
                                rs.getString("CR1_BUS_QLFY_ID"),
                                rs.getString("DDE_LINE1_NM"),
                                rs.getInt("CR1_NO_CA1")
                        ));
    }

    @Override
    public List<InputDto> getBicQualRows(int cr1Number, String region) {
        String sql = "";
        sql = "SELECT \n" +
                " SMG_TY_CD1\n" +
                ",CD1_FORMAT_CD\n" +
                ",PRF_CD_CD1\n" +
                ",SMV_CD_CD1\n" +
                ",CD1_EFF_DT\n" +
                ",CD1_EXP_DT\n" +
                ",CD1_BROKER_PRES_CD\n" +
                ",CD1_SEC_PRES_CD\n" +
                ",CD1_TEST_IN\n" +
                ",CD1_EVENT_DRIVE_IN\n" +
                ",CD1_NO\n" +
                ",CR1_NO_CD1\n" +
                ",'" + region + "' as region " +
                ",CD1_ACCT_XREF_CD\n" +
                "FROM " + region + ".CDV_EXTRACT1\n" +
                "WHERE CR1_NO_CD1 = " + cr1Number +
                " WITH UR";
//        System.out.println("sql abu1->" + sql);
        return jdbcTemplate.query(
                sql,
                (rs, rowNum) ->
                        new InputDto(
                                rs.getString("SMG_TY_CD1"),
                                rs.getString("CD1_FORMAT_CD"),
                                rs.getString("PRF_CD_CD1"),
                                rs.getString("SMV_CD_CD1"),
                                rs.getDate("CD1_EFF_DT"),
                                rs.getDate("CD1_EXP_DT"),
                                rs.getString("CD1_BROKER_PRES_CD"),
                                rs.getString("CD1_SEC_PRES_CD"),
                                rs.getString("CD1_TEST_IN"),
                                rs.getString("CD1_EVENT_DRIVE_IN"),
                                rs.getInt("CD1_NO"),
                                rs.getInt("CR1_NO_CD1"),
                                rs.getString("region"),
                                rs.getString("CD1_ACCT_XREF_CD")
                        ));
    }

    @Override
    public ViewUpdateDto getBicQualRow(String region, int cr1Number, int cd1Number) {
        String sql = "";
        sql = "SELECT \n" +
                " SMG_TY_CD1\n" +
                ",CD1_FORMAT_CD\n" +
                ",PRF_CD_CD1\n" +
                ",SMV_CD_CD1\n" +
                ",CD1_EFF_DT\n" +
                ",CD1_EXP_DT\n" +
                ",CD1_BROKER_PRES_CD\n" +
                ",CD1_SEC_PRES_CD\n" +
                ",CD1_TEST_IN\n" +
                ",CD1_EVENT_DRIVE_IN\n" +
                ",CD1_NO\n" +
                ",CR1_NO_CD1\n" +
                ",'" + region + "' as region " +
                ",CD1_CONTINGENCY_IN\n" +
                ",CD1_SEND_CD\n" +
                ",CD1_CASH_SUPP_IN\t\n" +
                ",CD1_INCR_IN       \n" +
                ",CD1_ACCTG_BASIS_CD " +
                ",CD1_PROJECTION_DS " +
                ",CDS_ID_CD1 " +
                ",CD1_HOLD_SUPP_IN " +
                ",CD1_ACCT_XREF_CD " +
                "FROM " + region + ".CDV_EXTRACT1\n" +
                "WHERE CR1_NO_CD1 = " + cr1Number +
                " AND CD1_NO =  " + cd1Number +
                " WITH UR";
        System.out.println("sql view/update sql->" + sql);
        return jdbcTemplate.queryForObject(
                sql,
                (rs, rowNum) ->
                        new ViewUpdateDto(
                                rs.getString("SMG_TY_CD1").substring(0, 6).trim(),
                                rs.getString("CD1_FORMAT_CD").trim(),
                                rs.getString("PRF_CD_CD1").trim(),
                                rs.getString("SMV_CD_CD1"),
                                rs.getDate("CD1_EFF_DT"),
                                rs.getDate("CD1_EXP_DT"),
                                rs.getString("CD1_BROKER_PRES_CD").trim(),
                                rs.getString("CD1_SEC_PRES_CD").trim(),
                                rs.getString("CD1_TEST_IN").trim(),
                                rs.getString("CD1_EVENT_DRIVE_IN").trim(),
                                rs.getInt("CD1_NO"),
                                rs.getInt("CR1_NO_CD1"),
                                rs.getString("region"),
                                rs.getString("CD1_CONTINGENCY_IN"),
                                rs.getString("CD1_SEND_CD"),
                                rs.getString("CD1_CASH_SUPP_IN"),
                                rs.getString("CD1_INCR_IN"),
                                rs.getString("CD1_ACCTG_BASIS_CD"),
                                rs.getInt("CD1_PROJECTION_DS"),
                                rs.getString("CDS_ID_CD1"),
                                rs.getString("CD1_HOLD_SUPP_IN"),
                                rs.getString("CD1_ACCT_XREF_CD")
                        ));
    }

    @Override
    public List<InputDto> getSecAccts(int cr1Number, String region) {
        String regiona = region.trim();
        String sql = "";

        sql = "SELECT ACC_NO_CS1,\n" +
                " SHORT_NAME\n" +
                ",STATUS_CODE\n" +
                "FROM " + regiona + ".CRG_SEC_ACCOUNT1\n" +
                "," + regiona + ".CUSTOMER_ACCOUNT\n" +
                "WHERE CR1_NO_CS1 = " + cr1Number +
                " AND NUMBER = ACC_NO_CS1\n" +
                "WITH UR;";
        System.out.println("sql sec act->" + sql);
        return jdbcTemplate.query(
                sql,
                (rs, rowNum) ->
                        new InputDto(
                                rs.getString("ACC_NO_CS1"),
                                rs.getString("SHORT_NAME"),
                                rs.getString("STATUS_CODE")
                        ));
//        return null;
    }
    public List<InputDto> getSecAcctsPos(int cr1Number, String region) {
        String regiona = region.trim();
        String sql = "";

//        sql = "SELECT ACC_NO_CS1,\n" +
//                " SHORT_NAME\n" +
//                ",STATUS_CODE\n" +
//                "FROM " + regiona + ".CRG_SEC_ACCOUNT1\n" +
//                "," + regiona + ".CUSTOMER_ACCOUNT\n" +
//                "WHERE CR1_NO_CS1 = " + cr1Number +
//                " AND NUMBER = ACC_NO_CS1\n" +
//                "WITH UR;";

        sql ="SELECT  ACC.NUMBER\n" +
                "      , ACC.LEGAL_TITLE_NAME\n" +
                "--     , ACC.FK_CUS_OWNED_BY\n" +
                "--      , CC1.CC1_CUSTODY_CD\n" +
                "FROM  " + regiona + ".CUSTOMER_ACCOUNT  ACC\n" +
                "    , " + regiona + ".CRG_CUSTOMER1     CC1\n" +
                "WHERE  CC1.CR1_NO_CC1       =  " + cr1Number +
                "  AND  ACC.FK_CUS_OWNED_BY  =  CC1.CUS_ID_CC1\n" +
                "  AND  ACC.STATUS_CODE   IN  ('ACT','OPN')\n" +
                "  AND NOT EXISTS\n" +
                "     ( SELECT *\n" +
                "       FROM    " + regiona + ".CRG_CUST_EXCL_ACC1  CE1\n" +
                "       WHERE  CE1.CR1_NO_CE1 = " + cr1Number +
                "         AND  CE1.CUS_ID_CE1 = CC1.CUS_ID_CC1\n" +
                "         AND  ACC_NO_CE1     = ACC.NUMBER)\n" +
                "ORDER BY  NUMBER\n" +
                "FOR FETCH ONLY\n" +
                "WITH UR";

        System.out.println("sql sec act pos->" + sql);
        return jdbcTemplate.query(
                sql,
                (rs, rowNum) ->
                        new InputDto(
                                rs.getString("NUMBER"),
                                rs.getString("LEGAL_TITLE_NAME")
//                                rs.getString("fk_cus_owned_by")
                        ));
//        return null;
    }


    @Override
    public List<InputDto> getCashAcct(int cr1Number, String region) {
        String regiona = region.trim();
        String sql = "";

        sql = "select A.SRC_CD_CA1 \n" +
                ",A.CAC_NO_CA1\n" +
                ",B.SHORT_NAME\n" +
                ",B.CLOSE_DATE\n" +
                "from " + regiona + ".CRG_CASH_ACCOUNT1 A\n" +
                "," + regiona + ".CASH_ACCOUNT\tB\n" +
                "where A.CR1_NO_CA1 = " + cr1Number + "\n" +
                "AND B.NUMBER = A.CAC_NO_CA1\n" +
                "AND B.SOURCE_SYSTEM_CODE = A.SRC_CD_CA1 \n" +
                "WITH UR;";
        System.out.println("sql cash act->" + sql);
        return jdbcTemplate.query(
                sql,
                (rs, rowNum) ->
                        new InputDto(
                                rs.getString("SRC_CD_CA1"),
                                rs.getString("CAC_NO_CA1"),
                                rs.getString("SHORT_NAME"),
                                rs.getString("CLOSE_DATE")
                        ));
    }

    @Override
    public List<InputDto> getSchedule(int cd1Number, String region) {
        String regiona = region.trim();
        String sql = "";

        sql = "SELECT CXS_NO, \n" +
                "CASE CXS_FREQUENCY_CD\n" +
                "     WHEN 'HR'\n" +
                "   THEN 'HOURLY'\n" +
                "WHEN 'DY'\n" +
                "   THEN 'HOURLY'\n" +
                "WHEN 'DI'\n" +
                "   THEN 'INTRDAY'\n" +
                "WHEN 'WK'\n" +
                "   THEN 'WEEKLY'\n" +
                "WHEN 'BI'\n" +
                "   THEN 'BIMONTLY'\n" +
                "WHEN 'MO'\n" +
                "   THEN 'MONTHLY'\n" +
                "WHEN 'MB'\n" +
                "   THEN 'MONTHLY GBD'\n" +
                "WHEN 'MA'\n" +
                "   THEN 'MONTHLY AS OF'\n" +
                "WHEN 'WM'\n" +
                "   THEN 'WEEKLY/MONTHLY'\n" +
                "WHEN 'QY'\n" +
                "   THEN 'QUARTERLY'\n" +
                "WHEN 'RQ'\n" +
                "   THEN 'ON REQUEST'\n" +
                "ELSE\n" +
                "   CXS_FREQUENCY_CD \n" +
                "END AS CXS_FREQUENCY_CD,\n" +
                "       CASE CXS_COB_IN\n" +
                "       WHEN '1' THEN 'Close of Business'\n" +
                "       ELSE CXS_COB_IN \n" +
                "       END AS CXS_COB_IN,\n" +
                "       CXS_EFFECTIVE_DT,\n" +
                "       CXS_DE       \n" +
                "FROM " + regiona + ".CLNT_EXT_SCHEDULE\n" +
                "    ," + regiona + ".CDV_EXT_SCHD_LINK1\n" +
                "WHERE CD1_NO_CL1       =  " + cd1Number + "\n" +
                "  AND CXS_NO           =  CXS_NO_CL1\n" +
                "ORDER BY CXS_COB_IN,\n" +
                "         CXS_FREQUENCY_CD,\n" +
                "         CXS_START_TM,\n" +
                "         CXS_DE,\n" +
                "         CXS_NO\n" +
                "         WITH UR;         ";
//        System.out.println("sql schedule->" + sql);
        return jdbcTemplate.query(
                sql,
                (rs, rowNum) ->
                        new InputDto(
                                rs.getInt("CXS_NO"),
                                rs.getString("CXS_FREQUENCY_CD"),
                                rs.getString("CXS_COB_IN"),
                                rs.getDate("CXS_EFFECTIVE_DT"),
                                rs.getString("CXS_DE")
                        ));
    }

    @Override
    public List<CategoryDto> getCategory(int cd1Number, String region) {
        String regiona = region.trim();
        String sql = "";

        sql = "SELECT PRF_CD_CF1 \t\t\t\n" +
                "      ,CXD_CD_CF1  \t\t\t\n" +
                "      ,CF1_EXT_PROCESS_ID  \t\n" +
                "      ,CF1_SEGMENT_CT\t\t\n" +
                "FROM " + regiona + ".CDV_EXT_FEATURE1\n" +
                "WHERE CD1_NO_CF1       =  " + cd1Number + "\n" +
                "ORDER BY CD1_NO_CF1       DESC \n" +
                "WITH UR;";

        System.out.println("sql category->" + sql);
        return jdbcTemplate.query(
                sql,
                (rs, rowNum) ->
                        new CategoryDto(
                                rs.getString("PRF_CD_CF1"),
                                rs.getString("CXD_CD_CF1"),
                                rs.getString("CF1_EXT_PROCESS_ID"),
                                rs.getInt("CF1_SEGMENT_CT")
                        ));
    }

    @Override
    public List<DestinationDto> getDestination(int cd1Number, String region) {
        String regiona = region.trim();
        String sql = "";


        sql = "  SELECT DDE_ID_DL1\n" +
                "      ,DDE_SRC_ID_DL1\n" +
                "      ,DL1_ACTION_CD\n" +
                "      ,DDE_LINE1_NM\n" +
                "FROM " + regiona + ".DLV_DEST_EXT_LINK1\n" +
                "      ," + regiona + ".DLV_DESTINATION\n" +
                "  WHERE CD1_NO_DL1   =  " + cd1Number + "\n" +
                "    AND DDE_TY       = DDE_TY_DL1\n" +
                "    AND DDE_ID_DL1   = DDE_ID\n" +
                "  ORDER BY DDE_ID_DL1 WITH UR;";

        System.out.println("sql destination->" + sql);
        return jdbcTemplate.query(
                sql,
                (rs, rowNum) ->
                        new DestinationDto(
                                rs.getString("DDE_ID_DL1"),
                                rs.getString("DDE_SRC_ID_DL1"),
                                rs.getString("DL1_ACTION_CD"),
                                rs.getString("DDE_LINE1_NM")
                        ));
    }

    @Override
    public List<InputDto> getOptDetail(int cr1Number, int cd1Number, String region) {
        String regiona = region.trim();
        String sql = "";

        sql = "SELECT  ROW_NUMBER() OVER() as row_count ,ACV_DE\n" +
                "          ,CASE SUBSTR(CD1_ADD_OPT_MAP,ROW_NUMBER() OVER(),1)\n" +
                "          \tWHEN '@' THEN ' ' \n" +
                "        \tELSE SUBSTR(CD1_ADD_OPT_MAP,ROW_NUMBER() OVER(),1)\n" +
                "        \tEND  AS  CD1_ADD_OPT_MAP     \t\n" +
                "       ,ACV_LONG_DE\n" +
                "  FROM  " + regiona + ".APPL_CODE_VALUE\n" +
                "       ," + regiona + ".CDV_EXTRACT1\n" +
                " WHERE  APC_CI_ACV = 'X31'\n" +
                "   AND  SUBSTR(ACV_CD,3,6) = SUBSTR(SMG_TY_CD1,1,6)\n" +
                "   AND CD1_NO     = " + cd1Number + " \n" +
                "   AND CR1_NO_CD1 = " + cr1Number + " \n" +
                "       WITH UR;";
        System.out.println("sql detail->" + sql);
        return jdbcTemplate.query(
                sql,
                (rs, rowNum) ->
                        new InputDto(
                                rs.getInt("row_count"),
                                rs.getString("ACV_DE"),
                                rs.getString("CD1_ADD_OPT_MAP"),
                                rs.getString("ACV_LONG_DE")
                        ));
    }

    @Override
    public int updateSetup(InputDto inputDto) {
        String sql = " ";
        sql = "UPDATE " + inputDto.getRegion() + ".CDV_EXTRACT1  \n" +
                "SET SMG_TY_CD1    = ?\n" +
                ",CD1_FORMAT_CD = ?\n" +
                ",PRF_CD_CD1\t = ?\n" +
                ",SMV_CD_CD1  = ?\n" +
                ",CD1_EFF_DT  = ?\n" +
                ",CD1_EXP_DT  = ?\n" +
                ",CD1_BROKER_PRES_CD  = ?\n" +
                ",CD1_SEC_PRES_CD  = ?\n" +
                ",CD1_TEST_IN  = ?\n" +
                ",CD1_EVENT_DRIVE_IN  = ?\n" +
                ",CD1_UPDATE_TS = CURRENT TIMESTAMP " +
                "WHERE CR1_NO_CD1 = ?\n" +
                "  AND CD1_NO = ?";

        System.out.println("update sql->" + sql);

        return jdbcTemplate.update(sql,
                inputDto.getMsgtype().trim(),
                inputDto.getFormatCd().trim(),
                inputDto.getProdCode().trim(),
                inputDto.getVariations().trim(),
                inputDto.getEffectiveDate(),
                inputDto.getExpireDate(),
                inputDto.getBrokerPresentCd().trim(),
                inputDto.getSecPresCd().trim(),
                inputDto.getTestInd().trim(),
                inputDto.getEventDriven().trim(),
                inputDto.getCr1Number(),
                inputDto.getCd1Number());
    }


}

